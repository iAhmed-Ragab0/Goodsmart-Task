      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Goodsmart Domain\Goodsmart Domain.csproj" />
    <ProjectReference Include="..\Goodsmart Repository\Goodsmart Repository.csproj" />
  </ItemGroup>

</Project>
